{
  "name": "@comunica/mediator-<%= name %>",
  "version": "1.0.0",
  "description": "A <%= name %> mediator",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/mediator-<%= name %>",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/mediator-<%= name %>/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/comunica/comunica/tree/master/packages/mediator-<%= name %>",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "runner"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://github.com/comunica/comunica#readme",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "@comunica/core": "^<%= versionComunicaCore %>"
  },
  "devDependencies": {
    "@comunica/core": "^<%= versionComunicaCore %>"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "prepare": "npm run build"
  }
}
